# ===========================================
# Seer Engine Configuration
# ===========================================

# OpenAI API (required for AI analysis)
OPENAI_API_KEY=your_openai_api_key

# Twitter API (from twitterapi.io)
# Leave empty to run in demo mode with mock tweets
TWITTER_API_KEY=your_twitter_api_key

# Opinion Trade (from opinion.trade)
# Leave empty to run in mock trading mode
OPINION_API_KEY=your_opinion_api_key
OPINION_PRIVATE_KEY=your_wallet_private_key_hex
OPINION_RPC_URL=https://bsc-dataseed.binance.org
OPINION_MULTISIG_ADDR=

# Server Configuration
BACKEND_PORT=3001
FRONTEND_URL=http://localhost:3000

# Demo Mode (set to 'true' to force demo mode even with API keys)
DEMO_MODE=false

